            "id": "6-bI_Zo_OW",
            "type": "code",
            "data": {
                "code": "<script>\n\n\/\/ ===========================\n\/\/ RENDER TOC\n\/\/ ===========================\nconst toc = document.getElementById(\"toc\");\nconst indicator = document.getElementById(\"indicator\");\n\nmoduleData.contents.forEach((c, i) => {\n    const item = document.createElement(\"div\");\n    item.classList.add(\"toc-item\");\n    item.dataset.target = c.id;\n    item.innerText = c.title;\n\n    \/\/ click \u2192 scroll to section\n    item.addEventListener(\"click\", () => {\n        document.getElementById(c.id).scrollIntoView({ behavior: \"smooth\" });\n    });\n\n    toc.appendChild(item);\n});\n\n\/\/ ===========================\n\/\/ RENDER CONTENT\n\/\/ ===========================\nconst contentArea = document.getElementById(\"content-area\");\n\nmoduleData.contents.forEach(c => {\n    const block = document.createElement(\"div\");\n    block.classList.add(\"content-block\");\n    block.id = c.id;\n\n    block.innerHTML = `\n        <h2>${c.title}<\/h2>\n        <p>${c.text}<\/p>\n    `;\n    contentArea.appendChild(block);\n});\n\n\/\/ ===========================\n\/\/ ACTIVE TRACKING + INDICATOR\n\/\/ ===========================\nconst tocItems = document.querySelectorAll(\".toc-item\");\nconst contentBlocks = document.querySelectorAll(\".content-block\");\nfunction updateActiveSection() {\n    let indexActive = 0;\n    contentBlocks.forEach((block, index) => {\n        const rect = block.getBoundingClientRect();\n        if (rect.top <= 120) {\n            indexActive = index;\n        }\n    });\n\n    tocItems.forEach(item => item.classList.remove(\"active\"));\n\n    const activeItem = tocItems[indexActive];\n    activeItem.classList.add(\"active\");\n\n    \/\/ move indicator \n    \/\/ const y = activeItem.offsetTop;\n    \/\/ const y = activeItem.offsetTop + activeItem.offsetHeight \/ 2;\n    const y = activeItem.offsetTop + activeItem.offsetHeight;\nindicator.style.height = y + \"px\";\n}\n\n\/\/ scroll event\nwindow.addEventListener(\"scroll\", updateActiveSection);\nupdateActiveSection();\n\n\/\/ ===========================\n\/\/ CONFETTI + FINISH DETECTOR\n\/\/ ===========================\nlet confettiTriggered = false;\nconst congratsBox = document.getElementById(\"congrats\");\n\nwindow.addEventListener(\"scroll\", () => {\n    const lastBlock = contentBlocks[contentBlocks.length - 1];\n    const rect = lastBlock.getBoundingClientRect();\n\n    if (rect.bottom <= window.innerHeight + 100) {\n        congratsBox.style.display = \"block\";\n\n        if (!confettiTriggered) {\n            confettiTriggered = true;\n            fireConfetti();\n        }\n    }\n});\n\nfunction fireConfetti() {\n    confetti({ particleCount: 160, spread: 90, origin: { y: 0.9 } });\n    setTimeout(() => confetti({ particleCount: 120, spread: 100, origin: { y: 0.7 } }), 600);\n    setTimeout(() => confetti({ particleCount: 190, spread: 110, origin: { y: 0.8 } }), 1200);\n}\n<\/script>"
            }
